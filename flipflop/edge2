#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x7fffea52f3e0 .scope module, "edge_tb" "edge_tb" 2 1;
 .timescale 0 0;
v0x7fffea54fad0_0 .var "cin", 0 0;
v0x7fffea54fb90_0 .var "clk", 0 0;
v0x7fffea54fc60_0 .net "cout", 0 0, L_0x7fffea54fe30;  1 drivers
v0x7fffea54fd60_0 .var "rstn", 0 0;
S_0x7fffea52f560 .scope module, "dut0" "edge_mod" 2 6, 3 1 0, S_0x7fffea52f3e0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "rstn"
    .port_info 2 /INPUT 1 "cin"
    .port_info 3 /OUTPUT 1 "cout"
L_0x7fffea54fe30 .functor BUFZ 1, v0x7fffea54f720_0, C4<0>, C4<0>, C4<0>;
v0x7fffea52f7a0_0 .net "cin", 0 0, v0x7fffea54fad0_0;  1 drivers
v0x7fffea54f720_0 .var "cin_f", 0 0;
v0x7fffea54f7e0_0 .net "clk", 0 0, v0x7fffea54fb90_0;  1 drivers
v0x7fffea54f880_0 .net "cout", 0 0, L_0x7fffea54fe30;  alias, 1 drivers
v0x7fffea54f940_0 .net "rstn", 0 0, v0x7fffea54fd60_0;  1 drivers
E_0x7fffea527740/0 .event negedge, v0x7fffea54f940_0;
E_0x7fffea527740/1 .event posedge, v0x7fffea54f7e0_0;
E_0x7fffea527740 .event/or E_0x7fffea527740/0, E_0x7fffea527740/1;
    .scope S_0x7fffea52f560;
T_0 ;
    %wait E_0x7fffea527740;
    %load/vec4 v0x7fffea54f940_0;
    %inv;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x7fffea54f720_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x7fffea52f7a0_0;
    %load/vec4 v0x7fffea54f720_0;
    %inv;
    %and;
    %assign/vec4 v0x7fffea54f720_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x7fffea52f3e0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffea54fb90_0, 0, 1;
T_1.0 ;
    %delay 5, 0;
    %load/vec4 v0x7fffea54fb90_0;
    %inv;
    %store/vec4 v0x7fffea54fb90_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x7fffea52f3e0;
T_2 ;
    %vpi_call 2 16 "$dumpfile", "edge.vcd" {0 0 0};
    %vpi_call 2 17 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x7fffea52f3e0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffea54fd60_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffea54fad0_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffea54fd60_0, 0, 1;
    %delay 500, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffea54fad0_0, 0, 1;
    %delay 500, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffea54fad0_0, 0, 1;
    %delay 500, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffea54fad0_0, 0, 1;
    %delay 500, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffea54fad0_0, 0, 1;
    %delay 500, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffea54fad0_0, 0, 1;
    %delay 5000, 0;
    %vpi_call 2 27 "$display", "\012*********FINISHING********\012" {0 0 0};
    %vpi_call 2 28 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
